models:
    Category: { name: string, description: 'text nullable', is_active: 'boolean nullable', parent_id: 'id foreign:categories.id nullable' }
    Currency: { code: string, symbol: string, name: 'string nullable', is_default: 'boolean default:false' }
    Location: { name: string, address: string, default_delivery_days: 'integer nullable' }
    Price: { amount: integer, seller_variant_id: 'id foreign', currency_id: 'id foreign' }
    SellerProduct: { name: string, sku: 'string nullable', description: 'text nullable', attributes: 'json nullable', category_id: 'id foreign' }
    SellerVariant: { name: string, sku: 'string nullable', description: 'text nullable', attributes: 'json nullable', seller_product_id: 'id foreign', status_id: 'id foreign' }
    Status: { name: 'enum:draft,pending_approval,approved,rejected,active,inactive,archived' }
    Stock: { quantity: integer, reserved: 'integer nullable', safety_stock: 'integer nullable', seller_variant_id: 'id foreign', location_id: 'id foreign' }
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
